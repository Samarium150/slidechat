{
	"name": "slidechat",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "PORT=10001 nodemon app",
		"prod": "PORT=10000 NODE_ENV=production nodemon app",
		"deploy": "npx pm2 start --env production",
		"build": "npm run build-c && npm run build-i",
		"build-c": "cd client && cross-env GENERATE_SOURCEMAP=false npm run build && cd ..",
		"build-i": "cd instructor-client && cross-env GENERATE_SOURCEMAP=false npm run build && cd .."
	},
	"dependencies": {
		"axios": "^0.21.1",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"connect-mongodb-session": "^2.3.3",
		"cookie-parser": "^1.4.4",
		"cors": "^2.8.5",
		"express": "^4.16.1",
		"express-fileupload": "^1.2.0",
		"express-session": "^1.17.1",
		"mongodb": "^3.5.7",
		"morgan": "^1.9.1",
		"pm2": "^4.4.0"
	},
	"devDependencies": {
		"@material-ui/core": "^4.2.0",
		"highlight.js": "^10.4.1",
		"markdown-it": "^11.0.0",
		"markdown-it-mathjax": "^2.0.0",
		"cross-env": "^7.0.3",
		"node-sass": "^4.14.1",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-router-dom": "^5.2.0",
		"nodemon": "^2.0.4"
	}
}
